{
	"registration": {
		"selector": "a#registration, .profile-links__register a",
		"value": "masthead:register:click"
	},
	"claimSubscribtion": {
		"selector": ".claimSubscribtion, .profile-links__claim a",
		"value": "masthead:claim-subscription:click"
	},
	"subscribe": {
		"selector": ".profileLink a:contains(Subscribe), .profile-links__subscribe a:contains(Subscribe)",
		"value": "masthead:subscribe:click"
	},
        "login" :{
              "selector" : ".profileLink a:contains(Login), a.ssoLogin, ul.profile-links:not(:has(div#login-links)) a:contains(Log in), ul.profile-links  li.article-tools__sub-item:last-child:contains(Login)",
              "value" : "masthead:login:click" 
        },
	"saveSearch": {
		"selector": "a.saveSearchLink",
		"value": "search:save-and-receive-alerts-for-this-search:click"
	},
	"createCitationAlert": {
		"selector": ".articleToolsContainer .toolAddCitation a, .article-tools__sub-item a.article-tools__item__displayCreateCitationAlert",
		"value": "article-tools:create-citation-alert:click"
	},
	"addToMyReadingList": {
		"selector": ".articleToolsContainer .toolReadingList a, .article-tools__sub-item a.article-tools__item__displayAddToMyReadingList",
		"value": "article-tools:add-to-my-reading-list:click"
	},
	"alertMeWithNewIssues": {
		"selector": ".alert-me-to-new-issue",
		"value": "issue:alert-me-with-new-issues:click"
	},
	"articleUpsellOrLoginCreateNewAccount": {
		"selector": ".ad_am_register a, .upsell-box__item a:contains(Create an account), .upsell-box__register.upsell-box__banner:contains(Register for free)",
		"value": "article-upsellorlogin:create-new-account:click"
	},
	"articleUpsellOrLoginAccessAny5Articles": {
		"selector": "a.ad_am_offer.allowanceOfferLink , .upsell-box__wrapper a:contains(Or purchase The Lancet Choice)",
		"value": "article-upsellorlogin:access-any-5-articles:click"
	},
	"articleUpsellOrLoginPurchaseThisArticle": {
		"selector": "a.ppvOfferLink , a.upsell-box__control:contains(Purchase one-time access),  a.upsell-box__action:contains(Purchase one-time access)",
		"value": "article-upsellorlogin:purchase-this-article:click"
	},
	"articleUpsellOrLoginPurchaseASubscription": {
		"selector": ".ad_am_SubscribeTxtCont a, .purchase-a-subscription, .upsell-box__banner.upsell-box__control:contains(Subscribe to)",
		"value": "article-upsellorlogin:purchase-a-subscription:click"
	},
	"articleUpsellOrLoginClaimOnlineAccess": {
		"selector": ".ad_am_subscriber a, .upsell-box__item a:contains(Claim online access), .claim-online-access",
		"value": "article-upsellorlogin:claim-online-access:click"
	},
	"articleUpsellOrLoginActivateYourFreeTrial": {
		"selector": ".ad_am_freeTrial a",
		"value": "article-upsellorlogin:activate-your-free-trial:click"
	},
        "ArticleUpsellLoginExistingAccount" : {
             "selector": ".insecure_replacement a:contains(Login to existing account), .upsell-box__actions__list a:contains(Log in to existing account), .upsell-box__item .upsell-box__already-subscriber",
             "value" : "article-upsellorlogin:login-existing-account:click"
         },
	"footerYourAccount": {
		"selector": ".page-footer ul li a:contains(Your Account), .rlist.clearfix.sided li a:contains(My Account)",
		"value": "footer-your-account:click"
	},
        "footerSubscriptionOptions2": {
		"selector": ".page-footer ul li a:contains(Existing Print Subscribers), .rlist.clearfix.sided li a:contains(Existing Print Subscribers)",
		"value": "footer-existing-print-subscribers:click"
	},
	"footerSubscriptionOptions1": {
		"selector": ".page-footer ul li a:contains(Subscription Options), .rlist.clearfix.sided li a:contains(Subscription Options), .rlist.clearfix li a:contains(Subscribe)",
		"value": "footer-subscription-options:click"
	},
        "footerRegistration": {
		"selector": ".rlist.clearfix li a:contains(Register)",
		"value": "footer-register:click"
	},
	"collectionCreateAlert": {
		"selector": ".collectionAlert",
		"value": "collection:create-alert:click"
	},
	"challengePageCreateAnAccount": {
		"selector": ".rightCol .CTABtn",
		"value": "challenge-page:create-an-account:click"
	},
	"ssoMemberLogin": {
		"selector": "a.acep-login, ul.profile-links li.article-tools__sub-item:first-child:contains(Member Login), .profile-links .profile-links__mda a:contains(Member Login)",
		"value": "sso:member-login:click"
	},
	"ssoInstantRegistrationClick": {
		"selector": ".ssoInstantRegistration a:contains(Instant Registration), .ssoInstantRegistration .sso-box a:contains(Continue)",
		"value": "sso:instant-registration:click"
	},
	"articleUpsellOrLoginCreateNewAccountLfm": {
		"selector": ".phone-ui .register",
		"value": "article-upsellorlogin:create-new-account:lfm:click"
	},
	"articleUpsellOrLoginPurchaseASubscriptionLfm": {
		"selector": ".phone-ui .subscriptionUrl:visible",
		"value": "article-upsellorlogin:purchase-a-subscription:lfm:click"
	},
	"articleUpsellOrLoginClaimOnlineAccessLfm": {
		"selector": ".phone-ui .claimAccessUrl:visible",
		"value": "article-upsellorlogin:claim-online-access:lfm:click"
	},
	"articleToolSubscription": {
		"selector": ".article-tools__subscribe a",
		"value": "article-tools:subscribe:click"
	},
        "articleToolPurchase": {
		"selector": ".article-tools__purchase a",
		"value": "article-tools:purchase-this-article:click"
	}
}
